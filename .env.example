# Example environment configuration
MONGODB_URI=mongodb://localhost:27017/ar
# Optional analytics endpoint
VITE_ANALYTICS_ENDPOINT=
# Optional external model URL
VITE_MODEL_URL=
# Base URL for Strapi CMS
VITE_STRAPI_URL=http://localhost:1337/api
# Allowed origins for Strapi CORS
FRONTEND_ORIGINS=http://localhost:5173
# Cloudflare R2 / S3 credentials
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_REGION=
R2_ENDPOINT=
R2_BUCKET=

# JWT secret for Strapi; generate with `openssl rand -hex 32`
JWT_SECRET=

# Port for API server (default 3000; Render.com uses 10000)
PORT=3000
